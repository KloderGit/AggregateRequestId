<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.3</Version>
    <Copyright>KloderGit</Copyright>
    <PackageProjectUrl>https://github.com/KloderGit/AggregateRequestId</PackageProjectUrl>
    <RepositoryUrl>https://github.com/KloderGit/AggregateRequestId</RepositoryUrl>
    <PackageTags>request</PackageTags>
    <PackageReleaseNotes>Get and set Aggregate-Request-Id header</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
  </ItemGroup>

</Project>
